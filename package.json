{
  "name": "@esolidar/toolkit",
  "version": "1.6.6",
  "description": "eSolidar toolkit",
  "main": "lib/index",
  "module": "es/index",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "test": "jest",
    "test:lint": "eslint ./src --ext .jsx --ext .js",
    "styleguide": "styleguidist server",
    "styleguide:build": "move-cli ./styleguide/.git ./src/.git && styleguidist build && move-cli ./src/.git ./styleguide/.git && cd ./styleguide && git checkout master && git add . && git commit -m \"automated deployment\"  && git push",
    "build": "nwb build-react-component --copy-files && rimraf lib/**/**/__tests__ && rimraf lib/**/**/*.md",
    "build:publish": "npm run build && npm publish",
    "clean": "nwb clean-module && nwb clean-demo",
    "lint": "react-scripts run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn test:lint"
    }
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "jest-localstorage-mock",
      "jest-date-mock"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "author": "Patr√≠cia Silva",
  "license": "MIT",
